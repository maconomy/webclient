<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
  
<suite name="Sanity" parallel="tests" thread-count="2" verbose="3" >
	<listeners>
		<listener class-name="utilities.TestListener"></listener>
	</listeners>
  <test name="Login" >
    <classes>
       <class name="tests.LoginTest" />
    </classes>
  </test>
   <test name="Main" >
    <classes>
       <class name="tests.MainTest" />
    </classes>
  </test>
</suite>
